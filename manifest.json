{
    "manifest_version": 2,

    "name": "Autofill",
    "description": "Extension autofill Supreme",
    "version": "1.0",
    
    "permissions": [
        "tabs",
        "https://www.supremenewyork.com/*",
        "storage"
    ],
    
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["https://www.supremenewyork.com/checkout"],
            "js": ["script.js"]
        }
    ]
        
}